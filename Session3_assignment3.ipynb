    "def main():\n",
    "    text_word = input(\"Type the word: \")\n",
    "\n",
    "    longest = 0\n",
    "\n",
    "    for words in text_word.split():\n",
    "        if len(words) > longest:\n",
    "            longest = len(words)\n",
    "            longest_word = words\n",
    "\n",
